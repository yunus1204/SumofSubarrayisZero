import java.util.*;
public class Main
{
	public static void main(String[] args) {
		int arr[]={4,2,-3,1,6};
		int n=arr.length;
		for(int i=0;i<n;i++){
		    for(int j=i;j<n;j++){
		        int sum=0;
		        for(int k=i;k<=j;k++){
		            sum+=arr[k];
		           
		        }
		       
		        if(sum==0){
		            System.out.println("yes");
		            break;
		        }
		    }
		}
	}
}
